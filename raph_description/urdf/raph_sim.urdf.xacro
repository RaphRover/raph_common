<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro"
       name="raph_sim">

  <xacro:arg name="fixed" default="false"/>
  <xacro:arg name="robot_ns" default=""/>

  <xacro:include filename="$(find raph_description)/urdf/raph_macros.xacro"/>

  <xacro:raph_sim robot_ns="$(arg robot_ns)"
                 fixed="$(arg fixed)"/>

</robot>